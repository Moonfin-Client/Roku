import "pkg:/source/api/MoonfinPlugin.bs"

sub init()
    m.top.functionName = "pingPlugin"
end sub

sub pingPlugin()
    result = moonfinPlugin.Ping()
    if isValid(result)
        result.addReplace("success", true)
        m.top.result = result
    else
        m.top.result = { success: false }
    end if
end sub
