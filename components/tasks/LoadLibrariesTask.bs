import "pkg:/source/api/sdk.bs"
import "pkg:/source/utils/misc.bs"

sub init()
    m.top.functionName = "loadLibrariesTask"
end sub

sub loadLibrariesTask()
    userId = m.top.userId
    if not isValidAndNotEmpty(userId) then return

    data = api.GetUserViews({ "userId": userId })
    if isValid(data)
        m.top.result = data
    end if
end sub
