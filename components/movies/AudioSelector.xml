<?xml version="1.0" encoding="utf-8"?>
<component name="AudioSelector" extends="Group">
    <children>
        <Rectangle width="1920" height="1080" color="#000000" opacity="0.75" />
        <Group translation="[100,100]">
            <Poster width="1720" height="880" uri="pkg:/images/dialog.9.png" />
            <LayoutGroup horizAlignment="center" translation="[860,50]" itemSpacings="[50]" layoutDirection="vert">
                <Text text="Select Audio Track" font="font:LargeBoldSystemFont" color="#FFFFFF" horizAlign="center" />
                <RadiobuttonList id="audioMenu" itemspacing="[0,10]" opacity="1" vertFocusAnimationStyle="floatingFocus" drawFocusFeedback="false" itemSize="[1500, 70]" numRows="10" />
            </LayoutGroup>
        </Group>
    </children>
    <interface>
        <field id="audioTracks" type="array" onChange="audioTracksSet" />
        <field id="audioStreamIndex" type="integer" />
        <field id="audioStreamName" type="string" />
    </interface>
</component>