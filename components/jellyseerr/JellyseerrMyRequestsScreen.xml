<?xml version="1.0" encoding="utf-8"?>
<component name="JellyseerrMyRequestsScreen" extends="JFScreen">

    <children>
        <Poster id="backgroundPoster" uri="pkg:/images/Overhang_Background_SD_2.jpg" width="1920" height="1080" opacity="0.3" />

        <Label id="titleLabel"
            text="My Requests"
            font="font:LargeSystemFont"
            translation="[60, 100]" />

        <RowList id="requestsRowList"
            translation="[60, 170]"
            width="1800"
            height="800"
            numRows="5"
            rowHeights="[200]"
            showRowLabel="[true]" />

        <Label id="noRequestsLabel"
            text="No requests yet"
            font="font:MediumSystemFont"
            translation="[960, 540]"
            horizAlign="center"
            vertAlign="center"
            visible="false" />

        <Label id="loadingLabel"
            text="Loading requests..."
            font="font:MediumSystemFont"
            translation="[960, 540]"
            horizAlign="center"
            vertAlign="center"
            visible="false" />
    </children>
</component>