<?xml version="1.0" encoding="utf-8"?>
<component name="HorizontalAlphaPicker" extends="Group">
    <children>
        <MarkupGrid
            id="alphaGrid"
            itemComponentName="AlphaItem"
            numRows="1"
            numColumns="28"
            itemSize="[40, 30]"
            itemSpacing="[2, 0]"
            drawFocusFeedback="true"
            focusBitmapUri="pkg:/images/white.png"
            focusFootprintBitmapUri=""
