                <ContentNode title="G" />
                <ContentNode title="H" />
                <ContentNode title="I" />
                <ContentNode title="J" />
                <ContentNode title="K" />
                <ContentNode title="L" />
                <ContentNode title="M" />
                <ContentNode title="N" />
                <ContentNode title="O" />
                <ContentNode title="P" />
                <ContentNode title="Q" />
                <ContentNode title="R" />
                <ContentNode title="S" />
                <ContentNode title="T" />
                <ContentNode title="U" />
                <ContentNode title="V" />
                <ContentNode title="W" />
                <ContentNode title="X" />
                <ContentNode title="Y" />
                <ContentNode title="Z" />
            </ContentNode>
        </MarkupGrid>
    </children>
    <interface>
        <field id="letterSelected" type="string" alwaysNotify="true" />
    </interface>
</component>