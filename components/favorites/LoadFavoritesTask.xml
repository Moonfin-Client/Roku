<?xml version="1.0" encoding="utf-8"?>
<component name="LoadFavoritesTask" extends="Task">
    <interface>
        <field id="filter" type="string" value="all" />
        <field id="limit" type="integer" value="100" />
        <field id="startIndex" type="integer" value="0" />
        <field id="results" type="array" />
        <field id="error" type="string" />
        <field id="totalCount" type="integer" value="0" />
        <!-- Multi-server support -->
        <field id="enableMultiServer" type="boolean" value="true" />
    </interface>
</component>